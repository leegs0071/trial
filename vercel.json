{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ],
  "env": {
    "API_KEY": "your-api-key",
    "DB_HOST": "localhost",
    "DB_PORT": 5432
  },
  "envSecrets": [
    "DB_PASSWORD"
  ],
  "public": true
}
